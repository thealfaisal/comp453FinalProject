### DDL

CREATE TABLE Customer
             (customerID          int    NOT NULL AUTO_INCREMENT,
	      username        VARCHAR(30)     NOT NULL UNIQUE,
	      firstName     VARCHAR(30) ,
              middleName        VARCHAR(30) ,
              lastName       VARCHAR(30) ,
              email  VARCHAR(30)  NOT NULL UNIQUE,
              password VARCHAR(100) NOT NULL,
              phoneNumber VARCHAR(30),
              Address VARCHAR(100),
              city VARCHAR(20),
              state VARCHAR(20),
              zipcode VARCHAR(20),
              admin boolean default false,
CONSTRAINT Customer_PK PRIMARY KEY (customerID));

ALTER TABLE Customer AUTO_INCREMENT=100001;

CREATE TABLE Location
             (locationID          int     NOT NULL AUTO_INCREMENT,
	      locationName        VARCHAR(30)     NOT NULL,
              city VARCHAR(20),
              state VARCHAR(20),
              zipcode VARCHAR(20),
CONSTRAINT Location_PK PRIMARY KEY (locationID));

ALTER TABLE Location AUTO_INCREMENT=101;

CREATE TABLE Vehicle
             (vehicleID          int     NOT NULL AUTO_INCREMENT,
	      vinNumber        VARCHAR(30) ,
	      engineNumber     VARCHAR(30) ,
              BrandName        VARCHAR(30) NOT NULL,
              image       VARCHAR(20) ,
              plateNumber  VARCHAR(20)  NOT NULL,
              ModelName VARCHAR(30) NOT NULL,
              Year int,
              style VARCHAR(30),
              transmission VARCHAR(20),
              trimLevel VARCHAR(15),
              mpg int,
              rate double,
              locationID int
CONSTRAINT Vehicle_PK PRIMARY KEY (vehicleID),
CONSTRAINT Loc_FK1 FOREIGN KEY (locationID)
	REFERENCES Location(locationID));

  ALTER TABLE Vehicle AUTO_INCREMENT=10001;


  CREATE TABLE Reservation
               (customerID          int     NOT NULL,
  	      vehicleID        int NOT NULL ,
                dateFrom        date ,
                dateTo       date ,
                timeFrom  time  ,
                timeTo time ,
                pickupLocation VARCHAR(50),
                dropoffLocation VARCHAR(50),
  CONSTRAINT Reserve_PK PRIMARY KEY (vehicleID,customerID),
  CONSTRAINT CustReserv_FK1 FOREIGN KEY (customerID)
  	REFERENCES Customer(customerID),
  CONSTRAINT VehReserv_FK1 FOREIGN KEY (vehicleID)
    	REFERENCES Vehicle(vehicleID));


App Functionality:
+ Search Page similar to Orbitz Car Lease Reservation (Customer fill pickuplocation, dropoffLocation, dateFrom, dateTo, timeTo and timeFrom)
+ Then a of vehicles based on search will be listed with vehicle info, rate and reserve button
+ Finally the customer will fill additional information and reserve

